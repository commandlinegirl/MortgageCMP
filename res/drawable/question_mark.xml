<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <color android:color="#FFEEEEEE" />
        
    <shape
       android:shape="oval">

    <gradient          
        android:startColor="#FF74C2E1"
        android:endColor="#FF63A5BF"         
        android:angle="270"/>
    
    <corners
     android:radius="5dp"/>
    
    <stroke 
        android:width="1dp" 
        android:color="#FF63A5BF" />
    
</shape>
        
    </item>
</selector>